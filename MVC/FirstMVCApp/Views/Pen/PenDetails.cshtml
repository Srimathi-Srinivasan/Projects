@{
}
@model IEnumerable<FirstMVCApp.Models.Pen>

    <table class="table">
        <tr>
            <td>PenID</td>
            <td>Pen Name</td>
            <td>Price</td>
        </tr>        

        @foreach(var item in Model)
        {
            <tr>
                <td>@item.PenId</td>
                <td>@item.PenName</td>
                <td>@item.Price</td>
            </tr>
        }
        
</table>
